{
  "compilerOptions": {
      "target": "es5",
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      // "noImplicitAny": true,
      // "allowJs": true,
      // "allowSyntheticDefaultImports": true,
      // "esModuleInterop": true,
      // "isolatedModules": true,
      // "jsx": "react",
      // "lib": ["es6"],
      // "moduleResolution": "node",
      // "composite": true,
      // "noEmit": true,
      // "strict": true,
      // "skipLibCheck": true,
      // "target": "esnext",
      "baseUrl": ".",
      "paths": {
          "@components/*": [
              "./src/components/*"
          ],
          "@views/*": [
              "./src/views/*"
          ],
          "@tabs/*": [
              "./src/components/Navigation/tabs/*"
          ],
          "@constants/*": [
              "./src/constants/*"
          ],
          "@utils/*": [
              "./src/utils/*"
          ],
          "@assets/*": [
              "./src/assets/*"
          ],
          "@libs/*": [
              "./src/libs/*"
          ],
          "@actions/*": [
              "./src/redux/actions/*"
          ],
          "@reducers/*": [
              "./src/redux/reducers/*"
          ],
          "@store": [
              "./src/redux/Store"
          ],
          "@saga/*": [
              "./src/redux/saga/*"
          ],
          "@repos/*": [
              "./src/redux/repos/*"
          ],
          "@redux/*": [
              "./src/redux/*"
          ],
          "@configs": [
              "./src/configs"
          ],
          "@types/*": [
              ".src/types/*"
          ]
      }
  },
  "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
  ],
  "include": [
      "**/*.ts",
      "**/*.tsx",
      "src/components/CityAndDistrict/data.ts"
, "src/components/fakeSever.js"    ]
}